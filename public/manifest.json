{
    "manifest_version": 3,
    "name": "posnFits",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
    },
    "background": {
      "service_worker": "background/background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.linkedin.com/*"],
        "js": ["content/content.js"]
      }
    ],
    "permissions": [
      "storage",
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "*://*.linkedin.com/*"
    ]
  }
  