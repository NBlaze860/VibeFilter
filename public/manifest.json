{
    "manifest_version": 3,
    "name": "posnFits",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background/background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/search/results/content/*"],
        "js": ["content/content.js"]
      }
    ],
    "permissions": [
      "storage",
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "https://www.linkedin.com/search/results/content/*"
    ]
  }
  